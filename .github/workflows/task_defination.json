{
    "family": "churn-prediction-task-defination",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::642902937696:role/ecsTaskExecutionRole",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "churn-prediction",
            "image": "642902937696.dkr.ecr.us-east-1.amazonaws.com/churn-prediction",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "name": "churn-prediction-8000-tcp",
                    "appProtocol": "http"
                }
            ],
            "environment": [
                {
                    "name": "ENV",
                    "value": "production"
                }
            ]
        }
    ]
}
