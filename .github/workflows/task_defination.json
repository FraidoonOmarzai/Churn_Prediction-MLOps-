{
    "family": "churn-prediction-task-definitions",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::642902937696:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "churn-prediction-container",
            "image": "642902937696.dkr.ecr.us-east-1.amazonaws.com/churn-prediction:latest",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/churn-prediction-logs",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
